@model List<SimpleECA.Models.Admin.CategoryViewModel>
<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Created Date</th>
            <th>Updated Date</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.categoryname</td>
                <td>@item.createdat.ToString("dd MMM yyyy")</td>
                <td>@item.updatedat.ToString("dd MMM yyyy")</td>
                <td>
                    <a href="#" onclick="openadminpartial('@Url.Action("GetCategoryById","Admin",new {id = item.categoryid })', 400)">Edit</a>/
                    <a href="#" onclick="openadminpartial('@Url.Action("DeleteCategoryById","Admin",new {id = item.categoryid })', 400)">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>