@model List<SimpleECA.Models.ProductViewModel>
<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Brand Name</th>
            <th>Category Name</th>
            <th>SubCategory Name</th>
            <th>Price</th>
            <th>Discount</th>
            @*<th></th>*@
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.productname</td>
                <td>@item.brandName</td>
                <td>@item.categoryName</td>
                <td>@item.subcatName</td>
                <td>@item.price</td>
                <td>@item.discount</td>
                @*<td>
                    <a href="#" onclick="openadminpartial('@Url.Action("GetProductById","Admin",new {id = item.productid })', 400)">Edit</a>/
                    <a href="#" onclick="openadminpartial('@Url.Action("DeleteProductById","Admin",new {id = item.productid })', 400)">Delete</a>
                </td>*@
            </tr>
        }
    </tbody>
</table>