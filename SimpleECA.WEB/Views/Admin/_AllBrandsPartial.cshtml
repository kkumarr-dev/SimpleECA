@model List<SimpleECA.Models.Admin.BrandViewModel>
<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>Brand Name</th>
            <th>Brand Description</th>
            <th>Created Date</th>
            <th>Updated Date</th>
            <th>Brand Logo</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.brandname</td>
                <td>@item.branddescription</td>
                <td>@item.createdat.ToString("dd MMM yyyy")</td>
                <td>@item.updatedat.ToString("dd MMM yyyy")</td>
                <td><img class="w-100" src="data:image/jpg;base64, @item.brandlogopath" /></td>
                <td>
                    <a href="#" onclick="openadminpartial('@Url.Action("GetBrandById","Admin",new {brandid = item.brandid })', 400)">Edit</a>/
                    <a href="#" onclick="openadminpartial('@Url.Action("DeleteBrandById","Admin",new {brandid = item.brandid })', 400)">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>